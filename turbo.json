{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "panel#build": {
      "dependsOn": ["^build"],
      "env": [
        "NODE_ENV",
        "BASE_URL",
        "SECRE_KEY",
        "STORYFLOW_PUBLIC_KEY",
        "STORYFLOW_PRIVATE_KEY",
        "EMAIL_SERVER_HOST",
        "EMAIL_SERVER_PORT",
        "EMAIL_SERVER_USER",
        "EMAIL_SERVER_PASS",
        "EMAIL_FROM",
        "S3_SECRET_KEY",
        "S3_ACCESS_KEY",
        "S3_ACCOUNT_ID",
        "S3_BUCKET_NAME",
        "OPEN_API_KEY",
        "UPSTASH_TOKEN",
        "PGCONNECTION"
      ],
      "outputs": [".next/**"]
    },
    "web#build": {
      "dependsOn": ["^build"],
      "env": [
        "NODE_ENV",
        "BASE_URL",
        "MONGO_URL",
        "NAMESPACES",
        "STORYFLOW_PUBLIC_KEY",
        "PUBLIC_KEY",
        "PRIVATE_KEY",
        "IMAGE_URL"
      ],
      "outputs": [".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "test": {}
  }
}
